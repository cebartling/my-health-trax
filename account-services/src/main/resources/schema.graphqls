type Query {
  bookById(id: ID): Book
  userAccountByEmail(email: String!): UserAccount
}

type Book {
  id: ID
  name: String
  pageCount: Int
  author: Author
}

type Author {
  id: ID
  firstName: String
  lastName: String
}

type UserAccount {
  id: ID!
  givenName: String!
  familyName: String!
  locale: String!
  email: String!
}